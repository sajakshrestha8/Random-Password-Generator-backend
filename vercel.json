{   
    "version": 2,
    "builds": [
        {
            "src": "./api/index.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/*.js": {
          "runtime": "nodejs18.x"
        }
}
